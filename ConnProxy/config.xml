<ProxyConfig>
	<Port>9696</Port>
	<!--<PassProxy>10.21.30.13:9696</PassProxy>-->
	<PrintLog>true</PrintLog>
	<PrintIpSummary>true</PrintIpSummary>
	<!--Allow Ip list etc 192.168.1.1,192.168.1.2,192.168.1.3-->
	<AllowIpStr></AllowIpStr>
	<!--buffer length-->
	<BuffSize>16384</BuffSize>
	<AllowMaxConn>10</AllowMaxConn>
	<!--buffer transform timeout by seconds-->
	<Timeout>10</Timeout>
</ProxyConfig>
